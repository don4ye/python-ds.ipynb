   ],
   "source": [
    "# Loop through rows\n",
    "for i in range(1, 6):\n",
    "    # Print increasing stars on the left side\n",
    "    print('*' * i, end='')\n",
    "\n",
    "    # Print spaces in the middle\n",
    "    print(' ' * (6 - i), end='')\n",
    "\n",
    "    # Print increasing stars on the right side\n",
    "    print('*' * i)\n"
   ]
